<Project Sdk="Microsoft.NET.Sdk">

  
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <!-- For NuGet -->
    <PackageId>ConnorWebSockets</PackageId>
    <Version>5.2.0</Version>
    <Authors>Connor Vargas</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/ConnorV645/ConnorWebSockets</RepositoryUrl>
    <PackageProjectUrl>https://github.com/ConnorV645/ConnorWebSockets</PackageProjectUrl>
    <PackageIcon>icon1.png</PackageIcon>
    <Description>Create a definable wrapper around WebSockets that allow for easy storage of related information.</Description>
    <AssemblyVersion>5.2.0.0</AssemblyVersion>
    <FileVersion>5.2.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Abstractions" Version="2.2.0" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="5.0.0" />
    <PackageReference Include="StackExchange.Redis" Version="2.2.4" />
    <None Include="icon1.png" Pack="true" PackagePath="\" />
  </ItemGroup>

</Project>
